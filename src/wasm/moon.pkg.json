{
  "import": [
    "mizchi/crdt_db/types",
    "mizchi/crdt_db/doc",
    "mizchi/crdt_db/merge",
    "mizchi/crdt_db/ephemeral",
    "moonbitlang/core/json"
  ],
  "supported-targets": ["wasm-gc", "js"],
  "link": {
    "wasm-gc": {
      "use-js-builtin-string": true,
      "exports": [
        "create_doc",
        "doc_insert",
        "doc_update",
        "doc_delete",
        "doc_merge_remote",
        "doc_get_pending",
        "doc_sync_state",
        "ephemeral_set",
        "ephemeral_get",
        "ephemeral_get_all",
        "ephemeral_merge"
      ]
    },
    "js": {
      "exports": [
        "create_doc",
        "doc_insert",
        "doc_update",
        "doc_delete",
        "doc_merge_remote",
        "doc_get_pending",
        "doc_sync_state",
        "ephemeral_set",
        "ephemeral_get",
        "ephemeral_get_all",
        "ephemeral_merge"
      ]
    }
  }
}
